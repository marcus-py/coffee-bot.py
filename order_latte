def order_latte(): 
  res = input('And what kind of milk for your latte? \n[a] 2% milk \n[b] Non-fat milk \n[c] Soy milk \n> ')
  if res == 'a':
    return 'latte'
    which_cup()
  elif res == 'b':
    return 'non-fat latte'
    which_cup()
  elif res == 'c':
    return 'soy latte'
    which_cup()
  else:
    error()
    return order_latte()
